<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC 
    "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.babyshow.upload.dao.ImageUploadDao">

	<insert id="insertImageUpload" parameterType="com.babyshow.upload.bean.ImageUpload">
		insert into
		t_imageupload(usercode,uploadtime,description,sharetype,sharetoken,result)
		value
		(#{userCode},#{uploadTime},#{description},#{shareType},#{shareToken},#{result})
	</insert>

</mapper>